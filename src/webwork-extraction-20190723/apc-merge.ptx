          <activity xml:id="act-changing-transformations-vert-stretch" xml:base="activities/act-changing-transformations-vert-stretch.xml">
            <statement>
              <p>
			Consider the functions <m>r</m> and <m>s</m> given in <xref ref="F-act-transformations-r-2">Figure</xref> and <xref ref="F-act-transformations-s-2">Figure</xref>.
		</p>
              <sidebyside width="50%">
                <figure xml:id="F-act-transformations-r-2">
                  <caption>A parent function <m>r</m>.</caption>
                  <image source="images/transformations-act-r-translation"/>
