          <activity xml:id="act-changing-transformations-translations" xml:base="activities/act-changing-transformations-translations.xml">
            <statement>
              <p>
			Consider the functions <m>r</m> and <m>s</m> given in <xref ref="F-act-transformations-r">Figure</xref> and <xref ref="F-act-transformations-s">Figure</xref>.
		</p>
              <sidebyside width="50%">
                <figure xml:id="F-act-transformations-r">
                  <caption>A parent function <m>r</m>.</caption>
                  <image source="images/transformations-act-r-translation"/>
                </figure>
                <figure xml:id="F-act-transformations-s">
